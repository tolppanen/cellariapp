<div class="demo-list-action mdl-list">
 <% @friends.each do |f|  %>
  <div class="mdl-list__item friend-list-item">
    <span class="mdl-list__item-primary-content">
      <i class="material-icons mdl-list__item-avatar">person</i>
      <span><%= f.username %></span>
    </span>
    <a class="mdl-list__item-secondary-action" href="#"><i class="material-icons">keyboard_arrow_right</i></a>
  </div>
 <% end %>
</div>



<h4>All Users</h4>
<ul>
<% @users.each do |u|  %>
    <li><i class="fi-torso"> </i><%= u.username %></li>
<% end %>
</ul>